    "# openai api 사용\n",
    "import openai\n",
    "import os\n",
    "import json\n",
    "\n",
    "# 프롬프트 입력받아서 응답 생성\n",
    "def get_response(prompt):\n",
    "    response = openai.Completion.create(\n",
    "        engine=\"gpt-4o-mini\",\n",
    "        prompt=prompt,\n",
    "        max_tokens=100\n",
    "    )\n",
    "    return response.choices[0].text.strip()\n"
   ]
  }
